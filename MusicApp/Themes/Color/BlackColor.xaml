    <!--当前播放列表-->
    <SolidColorBrush x:Key="PlayListTextColor" Color="#d6d6d6"/>
    <SolidColorBrush x:Key="PlayListAuxTextColor" Color="#5b5b5b"/>
    <SolidColorBrush x:Key="PlayListBoxBackColor" Color="#393939"/>
